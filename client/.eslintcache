witch (action.type) {\n    case FETCH_USER:\n      return action.payload || false;\n    default:\n      return state;\n  }\n}\n","/Users/westoncarpenter/Desktop/ReactApp/react_review/server/client/src/actions/types.js",[],"/Users/westoncarpenter/Desktop/ReactApp/react_review/server/client/src/actions/index.js",[],"/Users/westoncarpenter/Desktop/ReactApp/react_review/server/client/src/components/Landing.js",["113","114"],"/Users/westoncarpenter/Desktop/ReactApp/react_review/server/client/src/components/Payments.js",["115"],"/Users/westoncarpenter/Desktop/ReactApp/react_review/server/client/src/reducers/index.js",[],"/Users/westoncarpenter/Desktop/ReactApp/react_review/server/client/src/components/Dashboard.js",[],"/Users/westoncarpenter/Desktop/ReactApp/react_review/server/client/src/components/surveys/SurveyNew.js",[],"/Users/westoncarpenter/Desktop/ReactApp/react_review/server/client/src/components/surveys/SurveyForm.js",["116"],"// Shows a form for a user to add input\nimport _ from \"lodash\";\nimport React, { Component } from \"react\";\nimport { reduxForm, Field } from \"redux-form\";\nimport { Link } from \"react-router-dom\";\n\nimport SurveyField from \"./SurveyField\";\nimport validateEmails from \"../../utils/validateEmails\";\nimport formFields from \"./formFields\";\n\nimport { Button, Grid } from \"@material-ui/core\";\nimport BackIcon from \"@material-ui/icons/ArrowLeftOutlined\";\nimport NextIcon from \"@material-ui/icons/ArrowRight\";\n\nconst SurveyForm = (props) => {\n  const renderFields = () => {\n    return _.map(formFields, ({ label, name }) => {\n      return (\n        <Field\n          key={name}\n          component={SurveyField}\n          type=\"text\"\n          label={label}\n          name={name}\n        />\n      );\n    });\n  };\n  return (\n    <div>\n      <form onSubmit={props.handleSubmit(props.onSurveySubmit)}>\n        {renderFields()}\n        <Grid container alignItems=\"center\" justify=\"space-between\">\n        <Grid item>\n        <Link to=\"/surveys\">\n          <Button color=\"secondary\" ><BackIcon/> Cancel</Button>\n        </Link>\n        </Grid>\n        <Grid item>\n        <Button color=\"primary\" type=\"submit\">\n          Next <NextIcon/>\n        </Button>\n        </Grid>\n        </Grid>\n      </form>\n    </div>\n  );\n};\n\nfunction validate(values) {\n  const errors = {};\n\n  errors.recipients = validateEmails(values.recipients || \"\");\n\n  _.forEach(formFields, ({ name }) => {\n    if (!values[name]) {\n      errors[name] = \"You must provide a value\";\n    }\n  });\n  return errors\n}\n\nexport default reduxForm({\n  validate,\n  form: \"surveyForm\",\n  destroyOnUnmount: false,\n})(SurveyForm);\n","/Users/westoncarpenter/Desktop/ReactApp/react_review/server/client/src/components/surveys/SurveyField.js",[],"/Users/westoncarpenter/Desktop/ReactApp/react_review/server/client/src/utils/validateEmails.js",["117","118","119"],"const re = /^(([^<>()\\[\\]\\\\.,;:\\s@\"]+(\\.[^<>()\\[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/\n\nexport default (emails) => {\n  const invalidEmails = emails\n    .split(\",\")\n    .map((email) => email.trim())\n    .filter((email) => re.test(email) === false);\n\n\n  if (invalidEmails.length) {\n    return `These emails are invalid: ${invalidEmails}`;\n  }\n  return;\n};\n","/Users/westoncarpenter/Desktop/ReactApp/react_review/server/client/src/components/surveys/surveyFormReview.js",[],"/Users/westoncarpenter/Desktop/ReactApp/react_review/server/client/src/components/surveys/SurveyFormReview.js",["120"],"// shows user their form inputs for review\nimport _ from \"lodash\";\nimport React from \"react\";\nimport { connect } from \"react-redux\";\nimport { withRouter } from \"react-router-dom\";\n\nimport formFields from \"./formFields\";\nimport * as actions from \"../../actions\";\n\nimport { Typography, Button, Grid, Divider } from \"@material-ui/core\";\nimport BackIcon from \"@material-ui/icons/ArrowLeftOutlined\";\nimport MailIcon from \"@material-ui/icons/Mail\";\nimport { makeStyles } from \"@material-ui/core/styles\";\n\nconst useStyles = makeStyles((theme) => ({\n  divide: {\n    margin: \"20px\",\n  },\n}));\n\nfunction SurveyFormReview({ onCancel, formValues, submitSurvey, history }) {\n  const classes = useStyles();\n\n  const reviewFields = _.map(formFields, ({ name, label }) => {\n    return (\n      <Grid item key={name}>\n        <Typography variant=\"h6\">{label}</Typography>\n        <Typography variant=\"body1\">{formValues[name]}</Typography>\n      </Grid>\n    );\n  });\n  return (\n    <div>\n      <Grid container spacing={3} direction=\"column\">\n        <Grid item>\n          <Typography variant=\"h5\">Please confirm your entries:</Typography>\n        </Grid>\n        <Divider className={classes.divide} />\n        {reviewFields}\n        <Divider className={classes.divide} />\n      </Grid>\n\n      <Grid container spacing={3} direction=\"row\" justify=\"space-between\">\n        <Grid item>\n          <Button color=\"secondary\" onClick={onCancel}>\n            <BackIcon /> Back\n          </Button>\n        </Grid>\n        <Grid item>\n          <Button\n            color=\"primary\"\n            onClick={() => submitSurvey(formValues, history)}\n          >\n            Send Survey <i className=\"material-icons right\">email</i>\n          </Button>\n        </Grid>\n      </Grid>\n    </div>\n  );\n}\n\nfunction mapStateToProps(state) {\n  return { formValues: state.form.surveyForm.values };\n}\n\nexport default connect(mapStateToProps, actions)(withRouter(SurveyFormReview));\n","/Users/westoncarpenter/Desktop/ReactApp/react_review/server/client/src/components/surveys/formFields.js",["121"],"export default [\n    { label: \"Survey Title\", name: \"title\" },\n    { label: \"Subject Line\", name: \"subject\" },\n    { label: \"Email Body\", name: \"body\" },\n    { label: \"